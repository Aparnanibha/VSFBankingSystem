@model IEnumerable<VSFBankingSystem.Models.AddPayee>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BeneficiaryName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NickName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccountNumberNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BeneficiaryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NickName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccountNumberNavigation.AccountNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BeneficiaryAccountNumber">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BeneficiaryAccountNumber">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BeneficiaryAccountNumber">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
